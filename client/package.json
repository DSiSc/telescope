{
  "name": "telescope-ui",
  "version": "1.0.0",
  "description": "This project is based on light-bootstrap-dashboard, use react-bootstrap component by default, and powered by IceWork.",
  "license": "MIT",
  "keywords": [
    "ice-scaffold"
  ],
  "dependencies": {
    "@icedesign/base": "^0.2.5",
    "@icedesign/container": "^0.1.7",
    "@icedesign/form-binder": "^0.1.3",
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.15.0",
    "bootstrap": "3.3.7",
    "chartist": "0.10.1",
    "codemirror": "^5.39.0",
    "echarts": "^4.2.0-rc.2",
    "echarts-for-react": "^2.0.15-beta.0",
    "flag-icon-css": "^3.2.1",
    "font-awesome": "^4.7.0",
    "match-sorter": "^2.3.0",
    "material-ui": "^0.20.2",
    "moment-timezone": "^0.5.23",
    "prop-types": "^15.5.8",
    "react": "16.2.0",
    "react-bootstrap": "0.32.1",
    "react-bootstrap-dialog": "^0.10.0",
    "react-bootstrap-pagination": "^1.0.6",
    "react-bootstrap-table": "^4.3.1",
    "react-chartist": "0.13.1",
    "react-codemirror2": "^5.1.0",
    "react-cookies": "^0.1.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dialog": "^1.0.2",
    "react-dom": "16.2.0",
    "react-fontawesome": "^1.6.1",
    "react-intl": "^2.7.2",
    "react-js-pagination": "^3.0.2",
    "react-notification-system": "0.2.17",
    "react-redux": "^5.1.1",
    "react-router-dom": "4.2.2",
    "react-table": "^6.8.6",
    "reactstrap": "^6.5.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "superagent": "^3.8.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-react": "^7.5.1",
    "ice-skin-loader": "^0.1.2",
    "node-sass": "^4.10.0",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react-app-rewired": "^1.5.2",
    "react-scripts": "^1.1.4",
    "sass-loader": "^7.0.3",
    "webpack-plugin-import": "^0.1.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-app-rewired start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext '.js,.jsx' --fix"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.com",
    "access": "public"
  },
  "buildConfig": {
    "theme": "@icedesign/skin",
    "entry": "src/index.js"
  },
  "scaffoldConfig": {
    "type": "react",
    "name": "ice-light-bootstrap-dashboard",
    "title": "ICE Light Bootstrap Dashboard",
    "categories": [
      "Dashboard"
    ],
    "screenshot": "https://img.alicdn.com/tfs/TB1265Oay6guuRjy1XdXXaAwpXa-2840-1596.png"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "title": "telescope-ui"
}
